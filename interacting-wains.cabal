                    directory ==1.2.*,
                    filepath ==1.4.*,
                    lens ==4.12.*,
                    MonadRandom ==0.4.*,
                    mtl ==2.2.*,
                    random ==1.1.*,
                    som ==8.2.*,
                    split ==0.2.*,
                    test-framework ==0.8.*,
                    test-framework-hunit ==0.3.*,
                    test-framework-quickcheck2 ==0.3.*,
                    transformers ==0.4.*,
                    utf8-string ==1.*,
                    vector ==0.10.*,
                    QuickCheck ==2.8.*
  Other-modules:    ALife.Creatur.Wain.Interaction.ActionQC

